cigarette = input("Are you a cigarette addict older than 75 years old?")
chronic = input("Do you have a severe chronic disease?")
immune = input("Is your immune system too weak?")
if cigarette == "yes":
  cigarette = False
if chronic == "yes":
  chronic = False
if immune == "yes":
  immune = False
if cigarette and chronic and immune:
  print("You are not in risky group")
else:
  print("You are in risky group")
print(bool(cigarette))
print(bool(chronic))
print(bool(immune))
